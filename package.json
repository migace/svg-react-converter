{
  "name": "svg-react-converter",
  "version": "1.0.0",
  "description": "Tool for converting SVG files to React components with directory structure preservation",
  "main": "src/app.js",
  "bin": {
    "svg-react-converter": "./src/app.js"
  },
  "scripts": {
    "start": "node src/app.js --input ./icons --output ./icons-components --config ./svgr.config.js"
  },
  "keywords": [
    "svg",
    "react",
    "svgr",
    "icon",
    "component"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "@svgr/core": "^8.0.0",
    "@svgr/plugin-jsx": "^8.0.1",
    "@svgr/plugin-prettier": "^8.0.1",
    "@svgr/plugin-svgo": "^8.0.1",
    "cosmiconfig": "^8.2.0",
    "glob": "^10.3.3",
    "mkdirp": "^3.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.14"
  }
}
